!Xft.autohint: false

! ===[Misc Configuration]==========================
urxvt.geometry: 120x40
urxvt.visualBell: true
urxvt.saveLines: 0
urxvt.fading: 0
urxvt.cursorBlink: true
urxvt.loginShell: true

! ===[Extensions]==================================
urxvt.perl-ext-common:   default,clipboard,url-select
urxvt.keysym.M-v:        perl:clipboard:paste

urxvt.urlButton:         1
urxvt.matcher.button:    1
urxvt.underlineURLs:     true
urxvt.keysym.M-u:        perl:url-select:select_next

! ===[UI Config]===================================
urxvt.scrollBar: false
urxvt.scrollBar_right: false
!urxvt.scrollstyle: plain

! ===[Colorscheme]=================================
#include ".colors/dwm-colors"

! ===[Xft Configuration]===========================
Xft.dpi: 72
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintfull
Xft.lcdfilter: lcddefault

! ===[Font Configuration]==========================
urxvt.font: xft:Monospace:pixelsize=13
urxvt.boldFont: xft:Monospace:pixelsize=13:style=bold
urxvt.italicFont: xft:Monospace:pixelsize=13:style=italic
urxvt.boldItalicFont: xft:Monospace:pixelsize=13:style=italic

! ===[Extensions]==================================
urxvt.perl-lib:          /home/smerrill/.urxvt/urxvt-perls:/home/smerrill/.urxvt/urxvt-tabbedex
urxvt.keysym.M-c:        perl:clipboard:copy
urxvt.keysym.M-t:        \001c
urxvt.keysym.M-braceright: \001n
urxvt.keysym.M-braceleft:  \001p
urxvt.urlLauncher:       /usr/bin/chromium-browser

! Back to normal Linux copypasta.
urxvt.copyCommand:  xsel -ib
urxvt.pasteCommand: xsel -ob

